<p id="notice"><%= notice %></p>

<h1>Notifies</h1>

<table>
  <thead>
    <tr>
      <th>Entity name</th>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notifies.each do |notify| %>
      <tr>
        <td><%= notify.entity_name %></td>
        <td><%= notify.first_name %></td>
        <td><%= notify.last_name %></td>
        <td><%= notify.invest_date %></td>
        <td><%= notify.MATURITY_DATE %></td>
        <td><%= notify.NOTIFY_DATE %></td>
        <td><%= notify.note_type %></td>
        <td><%= notify.dollar_invest %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Notify', new_notify_path %>
