<p id="notice"><%= notice %></p>

<h1>Investors to Notify of Upcoming Maturity</h1>

<table id="notifications" name="notifications">
  <thead>
    <tr>
      <th>Rep </th>
      <th>Broker </th>
      <th>First name</th>
      <th>Last name</th>
      <th>Entity name</th>
      <th>Invest Date </th>
      <th>Maturity Date </th>
      <th>Notify Date </th>
      <th>Note Type </th>
      <th>Dollar Invest </th>
    </tr>
  </thead>

  <tbody>
    <% @notifies.each do |notify| %>
      <tr>
        <td><%= notify.rep %></td>
        <td><%= notify.broker %></td>
        <td><%= notify.first_name %></td>
        <td><%= notify.last_name %></td>
        <td><%= notify.entity_name %></td>
        <td><%= notify.invest_date %></td>
        <td><%= notify.MATURITY_DATE %></td>
        <td><%= notify.NOTIFY_DATE %></td>
        <td><%= notify.note_type %></td>
        <td><%= notify.dollar_invest %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>