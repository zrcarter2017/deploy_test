<%= form_for(distribution) do |f| %>
  <% if distribution.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(distribution.errors.count, "error") %> prohibited this distribution from being saved:</h2>

      <ul>
      <% distribution.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>


  <div class="field">
    <td><%= f.label :note_id %></td>
    <td><%= distribution.note_id %></td>
  </div>

  <div class="field">
    <td><%= f.label :quart_num %></td>
    <td><%= distribution.quart_num %></td>
  </div>

  <div class="field">
    <td><%= f.label :dist_date %></td>
    <td><%= distribution.dist_date %></td>
  </div>

  <div class="field">
    <td><%= f.label :cum_sum_days %></td>
    <td><%= distribution.cum_sum_days %></td>
  </div>

  <div class="field">
    <td><%= f.label :quart_days %></td>
    <td><%= distribution.quart_days %></td>
  </div>

  <div class="field">
    <td><%= f.label :dollar %></td>
    <td><%= distribution.dollar %></td>
  </div>

  <div class="field">
    <td><%= f.label :interest_rate %></td>
    <td><%= distribution.interest_rate %></td>
  </div>

  <div class="field">
    <td><%= f.label :distribution %></td>
    <td><%= distribution.distribution %></td>
  </div>

  <div class="field">
    <%= f.label :paid %>
    <%= f.select :paid, [['YES','YES'],['NO','NO']] %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
