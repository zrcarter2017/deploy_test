<p id="notice"><%= notice %></p>

<h1>Vizus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vizus.each do |vizu| %>
      <tr>
        <td><%= link_to 'Show', vizu %></td>
        <td><%= link_to 'Edit', edit_vizu_path(vizu) %></td>
        <td><%= link_to 'Destroy', vizu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vizu', new_vizu_path %>
