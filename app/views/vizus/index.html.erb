<%= javascript_include_tag "https://www.gstatic.com/charts/loader.js" %>

<h1>REVA Funding Visualizations</h1>

<br>
<h5> Count Note Types </h5>
<div align = "left" style="width:100%; height:200px">
  <%= bar_chart Note.group(:note_type).count %>
</div>

<h5> Distribution of Expirations </h5>
<div align = "right" style="width:100%; height:200px">
  <%= column_chart Vizu.group_by_quarter(:MATURITY_DATE).count %>
</div>

<h5> New Investors over Time </h5>
<div>
  <%= line_chart Investor.group_by_week(:created_at).count %>
</div>